<tr>
    <th colspan="3">
        <span>Общая сумма: {{round servicesTotalSum}}</span>
        <span> р.</span>
    </th>
</tr>
<tr>
    <th>Наименование</th>
    <th>Количество</th>
    <th>Сумма</th>
</tr>
{{#each servicesResult as |value key|}}
    <tr>
        <td>{{key}}</td>
        <td>{{value.count}} шт.</td>
        <td>{{round value.sum}} р.</td>
    </tr>
{{/each}}