<tr>
    <td colspan="4">
        <span>Общая сумма:</span>
        <span>{{round total}} р</span>
    </td>
</tr>
<tr>
    <th>Дата</th>
    <th>Причина</th>
    <th>Сумма, руб.</th>
    <th>Действие</th>
</tr>
{{#each details}}
    <tr class="js-tr">
        <td>{{date}}</td>
        <td>
            <a href="/car-reservation/{{id}}">{{description}}</a>
        </td>
        <td>{{round sum}} руб.</td>
        <td>
            <a href="javascript:void(0);">Удалить</a>
        </td>
    </tr>
{{/each}}